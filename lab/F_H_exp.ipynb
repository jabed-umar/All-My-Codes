    "# Creating n*n matrix without numpy\n",
    "# creating null matrix\n",
    "def matrix(n):\n",
    "    m = []\n",
    "    for i in range(n):\n",
    "        m.append([])\n",
    "        for j in range(n):\n",
    "            m[i].append(0)\n",
    "    return m\n",
    "matrix(10)"
